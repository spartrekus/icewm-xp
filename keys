

key "Super+e" rox

key "Super+s" xterm -e alsamixer 
key "Super+t"    xterm -bg black -fg yellow -fn DejaVuMono -fa 50 -fs 12 -e bash 
key "Super+x"    xterm -bg black -fg green -fn DejaVuMono -fa 50 -fs 12 -e bash 


key "Super+a"    wmctrl -r :ACTIVE: -b toggle,maximized_vert,maximized_horz 
key "Super+w"  wmctrl -c :ACTIVE: 
key "Super+q"   xdotool getactivewindow windowminimize 

key "Alt+Ctrl+t" xterm
key "Alt+Ctrl+f"  links -g google.com

key "Super+l"   xlock -mode daisy 

key "Super+n"   nedit

key "Print"   scrot '%Y%m%d%T-$wx$h-screenshot.png'  
key "Pause"   nconfig xip

key XF86AudioLowerVolume      amixer -q sset Master 3%-
key XF86AudioRaiseVolume      amixer -q sset Master 3%+
key XF86Calculator        x48 
key XF86Explorer          rox 

